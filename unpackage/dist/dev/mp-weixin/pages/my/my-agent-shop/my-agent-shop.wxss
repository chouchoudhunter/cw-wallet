.my-agent {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
  background-color: #ffffff;
  height: 148px;
  margin-bottom: 7px;
}
.my-agent .number-title {
  -webkit-flex-shrink: 0;
          flex-shrink: 0;
  text-align: center;
  width: 50%;
  margin: 20px 0;
}
.my-agent .number-title .number {
  font-size: 13px;
  color: #888888;
}
.my-agent .number-title .title {
  font-size: 15px;
}
.shop-list .shop {
  padding: 12px;
  margin-bottom: 1px;
  background-color: #ffffff;
  line-height: 2;
}
.shop-list .shop .shop-img-name {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  margin-bottom: 12px;
}
.shop-list .shop .shop-img-name .shop-img {
  -webkit-flex-shrink: 0;
          flex-shrink: 0;
  width: 120px;
  height: 90px;
  background-color: #C8C7CC;
  margin-right: 12px;
}
.shop-list .shop .shop-img-name .shop-name {
  font-size: 16px;
  line-height: 1.5;
}
.shop-list .shop .shop-address-tel {
  font-size: 14px;
}
.shop-list .shop .shop-money-date {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  font-size: 14px;
}
.shop-list .shop .shop-money-date .money {
  -webkit-box-flex: 1;
  -webkit-flex-grow: 1;
          flex-grow: 1;
}
.shop-list .shop .shop-money-date .money ._span {
  color: #FF2121;
}

